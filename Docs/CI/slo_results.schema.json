{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "slo_results",
  "type": "object",
  "properties": {
    "platform": {"type": "string"},
    "metrics": {
      "type": "object",
      "properties": {
        "ipc.rtt_same_core_p99_us": {"type": "number", "description": "IPC round-trip time same-core p99 (microseconds)"},
        "cap.revoke_block_new_p99_us": {"type": "number", "description": "Capability revoke block new p99 (microseconds)"},
        "cap.revoke_tear_maps_p99_ms": {"type": "number", "description": "Capability revoke tear shared maps p99 (milliseconds)"},
        "scheduler.input_p99_ms": {"type": "number", "description": "Scheduler input latency p99 (milliseconds)"},
        "scheduler.compositor_cpu_ms": {"type": "number", "description": "Compositor CPU time (milliseconds)"},
        "scheduler.audio_jitter_p99_us": {"type": "number", "description": "Audio jitter p99 (microseconds)"},
        "timer.deadline_jitter_p99_us": {"type": "number", "description": "Deadline timer jitter p99 (microseconds)"},
        "tsc.cross_core_skew_p99_us": {"type": "number", "description": "TSC cross-core skew p99 (microseconds)"},
        "tsc.monotonic_drift_ms_per_hour": {"type": "number", "description": "TSC monotonic drift (ms/hour)"},
        "ntp.offset_p99_ms": {"type": "number", "description": "NTP offset p99 (milliseconds)"},
        "ptp.offset_p99_us": {"type": "number", "description": "PTP offset p99 (microseconds)"},
        "power.idle_screen_on_w": {"type": "number", "description": "Power consumption idle screen on (watts)"},
        "power.idle_screen_off_w": {"type": "number", "description": "Power consumption idle screen off (watts)"},
        "power.s0ix_residency_pct": {"type": "number", "description": "S0ix residency percentage"},
        "power.resume_to_first_frame_ms": {"type": "number", "description": "Resume to first frame (milliseconds)"},
        "memory.anon_page_fault_p99_us": {"type": "number", "description": "Anonymous page fault service p99 (microseconds)"},
        "memory.tlb_shootdown_p99_us": {"type": "number", "description": "TLB shootdown p99 (microseconds)"},
        "psi.memory_stall_pct": {"type": "number", "description": "PSI memory stall percentage"},
        "trace.correlation_rate_pct": {"type": "number", "description": "Trace correlation rate percentage"},
        "minidump.generation_time_s": {"type": "number", "description": "Minidump generation time (seconds)"},
        "minidump.size_mb": {"type": "number", "description": "Minidump size (megabytes)"},
        "symbolization.job_time_s": {"type": "number", "description": "Symbolization job time (seconds)"},
        "fs.power_fail_cycles": {"type": "number", "description": "File system power-fail test cycles"},
        "fs.metadata_corruption_count": {"type": "number", "description": "File system metadata corruption count"},
        "nvme.4kib_qd1_p99_us": {"type": "number", "description": "NVMe 4KiB QD=1 p99 (microseconds)"},
        "nvme.flush_p99_us": {"type": "number", "description": "NVMe flush p99 (microseconds)"},
        "network.user_loopback_p99_us": {"type": "number", "description": "User-to-user loopback p99 RTT (microseconds)"},
        "tls.handshake_p95_ms": {"type": "number", "description": "TLS handshake p95 (milliseconds)"},
        "quic.establish_1rtt_p95_ms": {"type": "number", "description": "QUIC 1-RTT establish p95 (milliseconds)"},
        "quic.queueing_delay_p99_ms": {"type": "number", "description": "QUIC queueing delay p99 (milliseconds)"},
        "nic.tail_latency_improvement_pct": {"type": "number", "description": "NIC tail latency improvement percentage"},
        "graphics.jitter_p99_ms": {"type": "number", "description": "Graphics jitter p99 (milliseconds)"},
        "fuzz.continuous_hours": {"type": "number", "description": "Continuous fuzzing hours"},
        "fuzz.unreproducible_crashes": {"type": "number", "description": "Unreproducible crashes count"},
        "fault_injection.error_branches_pct": {"type": "number", "description": "Error branches exercised percentage"},
        "property_tests.generated_cases": {"type": "number", "description": "Property-based test generated cases"},
        "chaos.invariant_violations": {"type": "number", "description": "Chaos testing invariant violations"},
        "lockdep.warnings": {"type": "number", "description": "Lockdep warnings count"},
        "lockdep.contention_metrics": {"type": "number", "description": "Lock contention metrics"},
        "recovery.boot_time_s": {"type": "number", "description": "Recovery boot time (seconds)"},
        "recovery.restore_time_min": {"type": "number", "description": "Recovery restore time (minutes)"},
        "numa.remote_wakeups_pct": {"type": "number", "description": "NUMA remote wakeups percentage"},
        "numa.cross_numa_ipc_penalty": {"type": "number", "description": "Cross-NUMA IPC penalty multiplier"},
        "container.overhead_pct": {"type": "number", "description": "Container overhead percentage"},
        "vm.host_input_latency_degradation_ms": {"type": "number", "description": "VM host input latency degradation (milliseconds)"}
      },
      "patternProperties": {
        "^[a-z0-9_\\.]+$": {"type": "number"}
      },
      "additionalProperties": false
    }
  },
  "required": ["platform", "metrics"]
}

